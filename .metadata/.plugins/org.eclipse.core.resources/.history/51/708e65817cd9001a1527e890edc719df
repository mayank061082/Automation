package filereader;

import java.lang.*;

public class ED1 {
	int age;
	String name;
	
	public ED1(String name, int age) {

		this.name=name;
		this.age=age;
		System.out.println("calling constructor");
		public ED1(String name1, int age1) {
			name=name1;
			age=age1;
			System.out.println("inner cons");
			
			
		}
		
	}

	public static void main(String[] args) {
		ED1 e=new ED1("mayank",23);
		
	}



}
